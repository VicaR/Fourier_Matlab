function [V]=poverh(z,t)
V=zeros (10,11);
z=[0:0.1:1];
t=[0:0.01:0.1];
k=0;
j=0;
i=0;
for i=1:10
for j=1:11
for n=1:2:100
    k=k+1;
          Z(k)=Zfunk(n,z(j));   
          a(k)=integral(n);
          T(k)=Tfunk(n,t(i),z,a(k));
          v(k)=reshenie(Z(k),T(k));
end
V(i,j)=sum(v);
k=0;
end
end
V
end

